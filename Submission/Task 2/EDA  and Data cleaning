   "source": [
    "#We create a data frame with all records that have the missing value in at least one column between 'channel_sales' and 'origin_up'\n",
    "missing_df_channel_sales = client_df[client_df.channel_sales == \"MISSING\"] \n",
    "missing_df_origin_up = client_df[client_df.origin_up == \"MISSING\"]\n",
    "missing_df = pd.concat([missing_df_origin_up, missing_df_channel_sales]).drop_duplicates()"
